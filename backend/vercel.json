{
    "version": 2,
    "builds": [
        {
            "src": "backend/package.json",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["backend/src/**", "backend/db/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/dist/index.js"
        }
    ]
}